<view class="_div data-v-b814dec6">
  <view class="_div data-v-b814dec6 header">
    <view class="_div data-v-b814dec6 my" bindtap="gotoProfile">
      <view class="_div data-v-b814dec6 left">
        <image src="{{userInfo.avatarUrl||'/static/images/default.jpg'}}" alt class="_img data-v-b814dec6"></image>
      </view>
      <view class="_div data-v-b814dec6 right">
        <view wx:if="{{userInfo.nickName}}" class="_h4 data-v-b814dec6">{{userInfo.nickName}}</view>
        <button wx:else class="_button data-v-b814dec6 toLogin">登录/注册</button>
      </view>
    </view>
  </view>
  <view class="_div data-v-b814dec6 list">
    <view class="_ul data-v-b814dec6">
      <button class="_li data-v-b814dec6" style="margin-left: -22px;" open-type="getPhoneNumber" bindgetphonenumber="bindCellphone" wx:if="{{phoneBtnShow}}">
        <image src='/static/images/icons8-shopping-basket-star-48.png' alt class="_img data-v-b814dec6"></image>
        <label class="_span data-v-b814dec6">绑定手机号</label>
      </button>
      <view bindtap="invite" class="_li data-v-b814dec6">
        <image src="/static/images/icons8-share-100.png" alt class="_img data-v-b814dec6"></image>
        <label class="_span data-v-b814dec6">邀请朋友</label>
        <image src="/static/images/toRight.png" class="_img data-v-b814dec6 right" alt srcset></image>
      </view>
      <view bindtap="goIntegral" class="_li data-v-b814dec6">
        <image src="/static/images/icons8-shopping-basket-star-48.png" alt class="_img data-v-b814dec6"></image>
        <label class="_span data-v-b814dec6">我的积分</label>
        <image src="/static/images/toRight.png" class="_img data-v-b814dec6 right" alt srcset></image>
      </view>
      <view bindtap="openOther" class="_li data-v-b814dec6">
        <image src="/static/images/intivi.png" alt class="_img data-v-b814dec6"></image>
        <label class="_span data-v-b814dec6">其他工具</label>
        <image src="/static/images/toRight.png" class="_img data-v-b814dec6 right" alt srcset></image>
      </view>
      <view bindtap="clearCache" class="_li data-v-b814dec6">
        <image src="/static/images/clean.png" alt class="_img data-v-b814dec6"></image>
        <label class="_span data-v-b814dec6">清除缓存</label>
        <image src="/static/images/toRight.png" class="_img data-v-b814dec6 right" alt srcset></image>
      </view>
      <view bindtap="help" class="_li data-v-b814dec6">
        <image src="/static/images/help.png" alt class="_img data-v-b814dec6"></image>
        <label class="_span data-v-b814dec6">使用帮助</label>
        <image src="/static/images/toRight.png" class="_img data-v-b814dec6 right" alt srcset></image>
      </view>
      <view bindtap="aboutMe" class="_li data-v-b814dec6">
        <image src="/static/images/icons8-more-info-64.png" alt class="_img data-v-b814dec6"></image>
        <label class="_span data-v-b814dec6">关于我们</label>
        <image src="/static/images/toRight.png" class="_img data-v-b814dec6 right" alt srcset></image>
      </view>
    </view>
  </view>
</view>