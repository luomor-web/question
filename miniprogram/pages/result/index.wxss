  margin: 0 auto;
  font-size: 35rpx;

  color: blue;
  height: 100rpx;

}
.content {
  text-align: center;
  margin-top: 40rpx;
  padding: 40rpx 41rpx 0 46rpx;
  margin: 0 auto;
